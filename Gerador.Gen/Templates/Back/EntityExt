        {

        }

        public <#className#>(<#parametersRequired#>) :
            base(<#parametersRequiredToBase#>)
        {

        }

		public class <#className#>Factory : <#className#>FactoryBase
        {
            public <#className#> GetDefaultInstance(dynamic data, CurrentUser user)
